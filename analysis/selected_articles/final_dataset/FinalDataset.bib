	note = {{ZSCC}: 0000003},
	keywords = {Machine learning, Sales forecasting, Client retention, Prepaid unitary services},
	file = {Schaeffer_Rodriguez Sanchez_2020_Forecasting client retention — A machine-learning approach.pdf:files/11567/Schaeffer_Rodriguez Sanchez_2020_Forecasting client retention — A machine-learning approach.pdf:application/pdf;ScienceDirect Snapshot:files/11568/S0969698919302668.html:text/html}
}

@article{keramati_improved_2014,
	title = {Improved churn prediction in telecommunication industry using data mining techniques},
