	note = {{ZSCC}: 0000104},
	keywords = {Customer relationship management, Random forests, Customer churn prediction, Ensemble algorithms, {GAMens}, Online gambling},
	file = {Coussement_De Bock_2013_Customer churn prediction in the online gambling industry.pdf:files/11552/Coussement_De Bock_2013_Customer churn prediction in the online gambling industry.pdf:application/pdf;ScienceDirect Snapshot:files/11553/S0148296312003530.html:text/html}
}

@article{amin_customer_2017,
